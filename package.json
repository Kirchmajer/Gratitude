{
  "name": "gratitude-app",
  "version": "1.0.0",
  "description": "A full-stack application for practicing daily gratitude",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "test-app": "node test-app.js"
  },
  "keywords": [
    "gratitude",
    "mindfulness",
    "react",
    "express",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
